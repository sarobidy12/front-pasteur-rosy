{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve", // Add or update this line
    "incremental": true,
    "types": [
      "@testing-library/jest-dom"
    ],
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "tailwind.config.js",
    "__tests__/**/*.ts",
    "__tests__/**/*.tsx",
    "jest.setup.js",
    "**/*.ts",
    "jest.config.js"
, "__tests__/Component", "app/admin/Component/Input/Category", "app/evenement/[title]"  ],
  "exclude": [
    "node_modules"
  ]
}
